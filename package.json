  "scripts": {
    "ng": "ng",
    "run:all": "npm run start:shell && npm run start:remote && npm run vuejs-remote:dev",
    "start:shell": "ng serve angular-shell",
    "build:shell": "ng build angular-shell",
    "watch:shell": "ng build angular-shell --watch --configuration development",
    "test:shell": "ng test angular-shell",
    "start:remote": "ng serve angular-remote",
    "build:remote": "ng build angular-remote",
    "watch:remote": "ng build angular-remote --watch --configuration development",
    "test:remote": "ng test angular-remote",
    "start:vuejs": "vite --config apps/vuejs-remote/vite.config.ts",
    "vuejs-remote:build": "run-p type-check build-only",
    "preview:vuejs": "vite preview",
    "vuejs-remote:build-only": "vite --config apps/vuejs-remote/vite.config.ts build"
  },
